<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:dynamsoft="clr-namespace:Dynamsoft.CameraEnhancer.Maui;assembly=Dynamsoft.CameraEnhancer.Maui"
             x:Class="EditAndNormalize.EditorPage"
             Title="EditorPage">
    <AbsoluteLayout>
        <dynamsoft:ImageEditorView x:Name="editorView"
                             AbsoluteLayout.LayoutBounds="0,0,1,1"
                             AbsoluteLayout.LayoutFlags="All"></dynamsoft:ImageEditorView>
        <Button
                x:Name="NormalizeBtn"
                Text="Normalize"
                Clicked="OnNormalizeBtnClicked"
                             AbsoluteLayout.LayoutBounds="0.5,0.9,100,50"
                             AbsoluteLayout.LayoutFlags="PositionProportional"/>
    </AbsoluteLayout>
</ContentPage>
